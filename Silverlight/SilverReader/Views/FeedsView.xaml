<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:SilverReader"
	x:Class="SilverReader.FeedsView"
	d:DesignWidth="64" d:DesignHeight="48">
	<UserControl.Resources>
		<local:FeedsViewModel x:Key="FeedsViewModelDataSource" />
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" DataContext="{Binding Source={StaticResource FeedsViewModelDataSource}}"/>
</UserControl>
